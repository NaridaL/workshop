float dotGridGradient(vec2 cell, vec2 pos) {
    vec2 d = cell - pos;

    vec2 gradient = gradients[(int(cell.x) * 59 + int(cell.y)) % gradients.length()];
    return dot(gradient, d);
}

float perlin(vec2 xy) {
    float x0 = floor(xy.x);
    float x1 = x0 + 1.0;
    float y0 = floor(xy.y);
    float y1 = y0 + 1.0;

    // Interpolate between grid point gradients
    float n00 = dotGridGradient(vec2(x0, y0), xy);
    float n10 = dotGridGradient(vec2(x1, y0), xy);
    float ny0 = mix(n00, n10, xy.x - x0);

    float n01 = dotGridGradient(vec2(x0, y1), xy);
    float n11 = dotGridGradient(vec2(x1, y1), xy);
    float ny1 = mix(n01, n11, xy.x - x0);

    return mix(ny0, ny1, xy.y - y0);
}

